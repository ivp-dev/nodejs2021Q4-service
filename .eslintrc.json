{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:import/recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
   "settings": {
    "import/extensions": [
      ".js",
      ".json",
      ".ts"
    ],
    "import/resolver": {
      "typescript": {}
    }
  },
  "rules": {
    "import/no-unresolved": [ 2, { "caseSensitive": false }]
  }
}
